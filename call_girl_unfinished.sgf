function swingin(pic, start, end, x,y, scaler, scaler2, ease, fadetime, posit ){

var image = new Sprite(pic, Foreground);

image.fade(fadetime-100,fadetime-50,0,1);
image.scaleVec(fadetime-100,fadetime,scaler,0,scaler2,0.15);
image.move(ease,start, fadetime-100, x,y,x,y);
image.move(ease,fadetime-100, fadetime, x,y,x,y+posit);
image.move(ease,end, fadetime, x,y+posit,x,y+posit);

}

