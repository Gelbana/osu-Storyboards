function introup(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2,ease, fadein ){

var image = new Sprite(pic, Foreground);

image.rotate(start,inirotater);
image.scale(start,scaler);
image.move(ease,fadein, end, x,y,z,a);
image.move(ease,fadein-100, fadein, x,y+300,x,y);
image.move(ease,end, end+100,z,a,z,y-300);

image.rotate(start,end,inirotater,rotater);

image.scale(fadein,end,scaler,scaler2);

}

function introdown(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2,ease, fadein ){

var image = new Sprite(pic, Foreground);

image.rotate(start,inirotater);
image.scale(start,scaler);
image.move(ease,fadein, end, x,y,z,a);
image.move(ease,fadein-100, fadein, x,y-300,x,y);
image.move(ease,end, end+100,z,a,z,y+300);

image.rotate(start,end,inirotater,rotater);

image.scale(fadein,end,scaler,scaler2);

}
