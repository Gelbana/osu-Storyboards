 function campart(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2 ){

var image = new Sprite(pic, Foreground);


image.fade(start-90, start, 0,1);
image.scale(start-50,start,scaler+0.15, scaler);
image.rotate(start,inirotater);
image.scale(start,scaler);
image.move(start-100, end, x,y,z,a);

image.rotate(start,end,inirotater,rotater);

image.scale(start,end,scaler,scaler2);

}

function campart2(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2, ease ){

var image = new Sprite(pic, Foreground);




image.scale(start,scaler);
image.move(ease,start, end, x,y,z,a);

image.rotate(start,end,inirotater,rotater);

image.scale(start,end,scaler,scaler2);

}
function campartbg(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2, ease ){

var image = new Sprite(pic, Background);


image.scale(start,scaler);
image.move(ease,start, end, x,y,z,a);

image.rotate(start,end,inirotater,rotater);

image.scale(start,end,scaler,scaler2);

}

function campart3(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2, ease, r,g,b ){

var image = new Sprite(pic, Foreground);

image.scale(start,scaler);
image.color(start,r,g,b);
image.move(ease,start, end, x,y,z,a);

image.rotate(start,end,inirotater,rotater);

image.scale(start,end,scaler,scaler2);

}
function campartreal(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2,ease, fadein ){

var image = new Sprite(pic, Foreground);

image.fade(start-9000, 0);
image.fade(fadein-50,fadein,0,1);
image.scale(fadein-50,fadein,scaler+0.15, scaler);
image.rotate(start,inirotater);
image.scale(start,scaler);
image.move(ease,start, end, x,y,z,a);

image.rotate(start,end,inirotater,rotater);

image.scale(fadein,end,scaler,scaler2);

}

 
