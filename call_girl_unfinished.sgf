function campartmeasure(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2,ease, fadein ){

var image = new Sprite(pic, Foreground);

image.fade(start-9000, 0);
image.fade(fadein-50,fadein,0,1);

image.scaleVec(start-200,start-100,1,1,0.15,1);
image.rotate(start,inirotater);
image.scale(start,scaler);
image.move(ease,start, end, x,y,z,a);

image.rotate(start,end,inirotater,rotater);

image.scale(fadein,end,scaler,scaler2);

}

function campart2measure(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2, ease ){

var image = new Sprite(pic, Foreground);

image.scale(start,0.15);
image.scaleVec(start,end,scaler,1,scaler2,1);
image.move(ease,start, end, x,y,z,a);

image.rotate(start,end,inirotater,rotater);



}


