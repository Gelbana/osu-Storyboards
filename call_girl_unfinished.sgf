function flyingenleft(pic,rote,rote2,start,end,scaler,scaler2,ease, offset, startpos){

var image = new Sprite(pic, Foreground);


var i=start;
var t=startpos;
var n=end;
for (;i<=n;i++){



	var y =((0.1*t)-offset)*((0.1*t)-offset)+150;
	var t2=t+1;
	var y2=((0.1*t2)-offset)*((0.1*t2)-offset)+150;
	t++;

image.scale(i,scaler);
image.move(ease,i,i+1, t,y,t2,y2);

image.rotate(i,n,rote,rote2);

image.scale(i,i+1,scaler,scaler2);




	 }
	 }
	 

