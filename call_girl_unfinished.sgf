function shake(pic,inirotater, rotater, start, end, x,y,z,a, scaler, scaler2,ease, fadein ){

var image = new Sprite(pic, Foreground);
var time=fadein-50;

var count=0;
image.fade(start-9000, 0);
image.fade(fadein-50,fadein,0,1);
image.scale(fadein-50,fadein,scaler+0.30, scaler);
image.rotate(start,inirotater);
image.scale(start,scaler);
image.rotate(start,end,inirotater,rotater);

image.scale(fadein,end,scaler,scaler2);
image.move(ease,start,time, x,y,z,a);


image.move(time,time+10,x,y,x+10,y-10);

image.move(time+10,time+20,x,y,x-20,y);

image.move(time+20,time+30,x,y,x+10,y-10);

image.move(time+30,time+50,x,y,x,y+20);
image.move(time+50,end,x,y,x,y);
}

