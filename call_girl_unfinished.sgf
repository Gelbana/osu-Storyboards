function camera1(pic, rotater, start, middle,end, x,y,z,a, speed){

var image = new Sprite(pic, Background);

image.move(start, middle, x,y,x,y);
image.move(middle, end, x,y,z,a);
image.rotate(middle,end,0,rotater);
image.scale(start,1);

image.scaleVec(1.5,middle,end,1,1,1.8,2.5);

image.move(end,end+2000, z,a,z,a);
}
function camera2(pic, rotater, start, middle,end, x,y,z,a, speed){

var image = new Sprite(pic, Foreground);

image.move(start, middle, x,y,x,y);
image.move(middle, end, x,y,z,a);
image.rotate(middle,end,0,rotater);
image.scale(start,1);

image.scaleVec(1.5,middle,end,1,1,1.8,2.5);

image.move(end,end+2000, z,a,z,a);
}

