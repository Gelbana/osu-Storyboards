function circlein(pic,start,end,r,g,b,scalee){
var circle=new Sprite(pic,Background);
circle.scale(start,end,0,scalee);
circle.color(start,r,g,b);
circle.move(start,end,320,240,320,240);
}

function circleout(pic,start,end,r,g,b,scalee){
var circle=new Sprite(pic,Background);
circle.scale(start,end,scalee,0);
circle.color(start,r,g,b);
circle.move(start,end,320,240,320,240);
}
