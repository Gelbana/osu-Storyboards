function cooltop(char, start,end, x,y ,z ,a, speed){
var symbol = new Sprite(char, Foreground);


var rotater = rand(0,360);
var rot=rand(-2,2);
	symbol.scale(start-150,start,0.001,0.5);
	symbol.move(start, end-162, x,y,x,y);
	symbol.rotate(start,end-162,rotater,rotater+rot);
	symbol.move(end-162,end,x,y,z,a);
	symbol.move(end,end+2595-200,z,a,z+speed,a);
	symbol.rotate(end-162,end,rotater+rot,0);
	symbol.scaleVec(end+2595-200,end+2595,1,1,1.5,1.5);
	symbol.rotate(end+2565-200,end+2595,0,rotater);
	symbol.move(end+2595-200,end+2595,z+speed,a, rand(0,600),rand(0,500));
	symbol.fade(end+2595-200,end+2595,1,0);
}


