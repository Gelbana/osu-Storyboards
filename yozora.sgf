function star(start){
	var star = new Sprite("sb/star.png");
	
		star.move(start,rand(0,1366),rand(0,768));
		star.scale(start-234,start, 0.001, 0.5);
		star.scale(start,start+243, 0.5, 0.001);
		star.fade(start+12,100);
		
}

function fadein(char,start,end, x, y){

var symbol = new Sprite(char, Foreground);

	symbol.move(start,end,x,y,x,y);
	symbol.scale(start,0.4);
	symbol.fade(start-150,start,0.001,1);
	symbol.fade(end-150,end,1,0);
	
	}

