function realfade(char, start,end, x,y, speed){
var symbol = new Sprite(char, Foreground);

	symbol.move(start,end,x,y,x+speed,y);
	symbol.fade(start-100,start+100,0.1,0.8);
	symbol.scale(start,0.6);
	symbol.fade(end-100,end+100,0.8,0);


}
