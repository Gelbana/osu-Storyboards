function chorusup1(char,start,midle,middle,x,y, speed,scaler){

var symbol = new Sprite(char, Foreground);

	symbol.move(start,midle,x,y,x,y);
	symbol.move(midle, middle,x,y,x,y+speed);	
	symbol.fade(start-150,start,0.001,1);
	symbol.scale(start,scaler);
	symbol.move(middle,middle+2594,x,y+speed,x+rand(-30,30),y+rand(-100,-40));
	symbol.scaleVec(middle,middle+1297,1,1,-0.75,0.75);
	symbol.scaleVec(middle+1297,middle+2594,-0.75,0.75,0.5,0.5);
	symbol.fade(middle+2594-2000,middle+2100,1,0);

}

